pontos_time1 = 0
pontos_time2 = 0

while True:
    ponto = int(input())
    if ponto == 1:
        pontos_time1 += 1
    elif ponto == 2:
        pontos_time2 += 1

    if (pontos_time1 >= 25 or pontos_time2 >= 25) and (pontos_time1 - pontos_time2 >= 2 or pontos_time2 - pontos_time1 >= 2):
        print(pontos_time1, 'x', pontos_time2)
        break
